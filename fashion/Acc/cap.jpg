              logEventToNewRelic(config.events.mailingConflict);
              logEventToAdobe(config.events.mailingConflict);

              elements.loading.classList.add("nst-is-hidden");
              elements.mailingConflict.classList.remove("nst-is-hidden");
              window.localStorage.setItem(config.storageKeys.completed, "true");

              setFocusTrap('nst-conflict-mailing');
              return;
            }
          }
        }

        logEventToNewRelic(config.events.error, { reason: "Uncaught" });
        elements.loading.classList.add("nst-is-hidden");
        elements.error.classList.remove("nst-is-hidden");
