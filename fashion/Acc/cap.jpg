      elements.form.addEventListener('submit', function (event) {
        event.preventDefault();
        elements.landing.classList.add("nst-is-hidden");
        elements.loading.classList.remove("nst-is-hidden");
        postForm();
      });

      setFocusTrap('nst-landing');
      window.addEventListener('keydown', keyboardListener);
    }

    // Trap the focus within the modal
    function setFocusTrap(page) {
      var closeButton = document.getElementById(page + "-close");
      closeButton.addEventListener('click', dismissModal);

      var focusableEls = document.getElementById(page).querySelectorAll('a[href], area[href], input:not([disabled]), select:not([disabled]), textarea:not([disabled]), button:not([disabled]), [tabindex="0"]');
      elements.focusable = focusableEls;
      elements.firstFocusable = focusableEls[0];
      elements.lastFocusable = focusableEls[focusableEls.length - 1];

      window.setTimeout(function() {
        elements.firstFocusable.focus();
      }, 300);
    }

    // Set a keyboard listener
    function keyboardListener(event) {
      var tabKey = 9;
      var escapeKey = 27;

      function handleBackwardTab() {
        if (document.activeElement === elements.firstFocusable) {
          event.preventDefault();
          elements.lastFocusable.focus();
        }
      }

      function handleForwardTab() {
        if (document.activeElement === elements.lastFocusable) {
          event.preventDefault();
          elements.firstFocusable.focus();
        }
      }

      if (event.keyCode === escapeKey) {
        event.preventDefault();
        dismissModal();
      }

      if (event.keyCode === tabKey) {
        if (elements.focusable.length === 1) {
          event.preventDefault();
          return;
        }

        if (event.shiftKey) {
          handleBackwardTab();
        } else {
          handleForwardTab();
        }
      }
    }

