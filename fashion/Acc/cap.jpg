    // Get or createcounter from Local storage
    function getOrCreateCounter() {
      var counter = window.localStorage.getItem(config.storageKeys.counter);
      if (!counter) {
        counter = 1;
        window.localStorage.setItem(config.storageKeys.counter, counter);
      }
