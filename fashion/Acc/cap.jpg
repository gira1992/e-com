        JSON.stringify({
          value: "true",
          expires: expirationTime
        })
      );
    }

    // Get the dismissed value - Return true or false if token is valid
    function getDismissed() {
      var data = JSON.parse(window.localStorage.getItem(config.storageKeys.dismissed));
      if (data) {
        var currentTime = new Date().getTime();
        if (currentTime > data.expires) {
          window.localStorage.removeItem(config.storageKeys.dismissed);
          window.localStorage.removeItem(config.storageKeys.counter);
          return false;
        }
        return true;
